{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true,
        "jest": true
    },
    "extends": "google",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "require-jsdoc": "off",
        "linebreak-style": "off",
        "indent": ["error", 2],
        "max-len": [1, 165, 2],
        "object-curly-spacing": [2, "always"],
        "no-trailing-spaces" : [
            "error", 
            { "skipBlankLines": true }
        ]
    }
}
